% -------------------- SURFACES --------------------

surf tvs_hex	        hexyc	0.0  0.0  11.6625  	% Shroud tube inner radius

surf tvel_bottom        pz 	    -188.5	            %tvel bottom
surf tvel_top           pz  	210.7	            %tvel top

surf tvs_bottom         pz 	    -215.2              %tvs bottom
surf tvs_top            pz  	249.6	            %tvs top

surf fuel_top           pz  	 184.0 	            %fuel top
surf fuel_bottom        pz 	    -184.0	            %fuel bottom
surf fuel_surf1    pz  	0.0	        %fuel top

% -------------------- CELLS --------------------

cell  tvel_bottom_zone      0   fill lt         -tvs_hex    tvel_bottom     -fuel_bottom	% tvel bottom lattice
cell  tvel_top_zone         0   fill lt         -tvs_hex    fuel_top        -tvel_top	    % tvel top lattice
cell  tvs_bottom_zone       0   wst             -tvs_hex    tvs_bottom      -tvel_bottom    % tvs bottom
cell  tvs_top_zone          0   wst             -tvs_hex    tvel_top        -tvs_top        % tvs top

cell 99  0  outside                     tvs_hex 		                            % Outside world
cell 98  0  outside                    -tvs_hex    -tvs_bottom		                % Outside world
cell 97  0  outside                    -tvs_hex     tvs_top		                    % Outside world
    cell  tvel_section_1     0  fill l1 -tvs_hex     fuel_bottom        -fuel_surf_1	    % tvel section 1
cell  tvel_section_2     0  fill l2 -tvs_hex     fuel_surf_1        -fuel_top	    % tvel section 2




% --- Thermal scattering data for light water:

therm lwtr lwj3.11t

% --- Cross section library file path:

set acelib "/home/viti/serpent/xs/jeff311/jeff311u.xsdata"
set nfylib "/home/viti/serpent/xs/jeff311/sss_jeff311.nfy"
set declib "/home/viti/serpent/xs/jeff311/sss_jeff311.dec"


% --- Periodic boundary condition:

set bc 3

% --- Group constant generation:

% universe = 0 (homogenization over all space)
% symmetry = 12
% 2-group structure (group boundary at 0.625 eV)

set gcu  0
set sym  12
set nfg  2  0.625E-6

% --- Neutron population and criticality cycles:

set pop 5000 20 20

% --- Geometry and mesh plots:

plot 3 5000 5000
plot 2 5000 5000 0 -250 300
mesh 2 5000 5000
mesh 3 5000 5000
